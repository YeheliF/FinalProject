{
    "name":"Plugin for Kayak website",
    "description":"Plugin for our final project - flight postponed",
    "version":"1.0",
    "manifest_version":2,
    
    "content_scripts": [

        {
            "matches":[
                "*://www.il.kayak.com/flights/*"  
            ],
            "js": ["jquery.min.js","sweetalert2.all.min.js", "content.js"],
            "css": ["style.css"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": [
            "jquery.min.js","sweetalert2.all.min.js","./content.js"
        ],
        "css": ["style.css"],
        "run_at": "document_end"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "https://www.il.kayak.com/flights/*",
        "http://www.il.kayak.com/flights/*",
        "storage"
    ]
    
}